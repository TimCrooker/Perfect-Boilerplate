{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"outDir": "./dist",
		"declaration": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": false,
		"moduleResolution": "node",
		"baseUrl": "./src",
		"paths": {
			"@Utils": [
				"./Utils"
			],
			"@Utils/*": [
				"./Utils/*"
			],
		},
		"types": [
			"node",
			"jest"
		],
		"esModuleInterop": true
	},
	"include": [
		"src/"
	],
	"exclude": [
		"template/",
		"lib/",
		"plugins/",
		"test/",
		"docs/"
	],
}